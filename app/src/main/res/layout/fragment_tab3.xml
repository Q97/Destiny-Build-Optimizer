<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/app_background"
    tools:context="com.example.destinybuildoptimizer.tab3">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/mobility_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="88dp"
            android:layout_marginEnd="36dp"
            android:background="?android:selectableItemBackground"
            android:foreground="@drawable/mobility"
            app:layout_constraintEnd_toStartOf="@+id/helmet_tab3"
            app:layout_constraintStart_toEndOf="@+id/mobilityvalue_tab3"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mobilityvalue_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="88dp"
            android:fontFamily="monospace"
            android:text="40"
            android:textColor="@color/DBOTextColor"
            android:textSize="36sp"
            app:layout_constraintEnd_toStartOf="@+id/mobility_tab3"
            app:layout_constraintStart_toEndOf="@id/classicontab3"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/resilience_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="36dp"
            android:background="?android:selectableItemBackground"
            android:foreground="@drawable/resilience"
            app:layout_constraintEnd_toStartOf="@id/arms_tab3"
            app:layout_constraintStart_toEndOf="@id/resiliencevalue_tab3"
            app:layout_constraintTop_toBottomOf="@id/mobility_tab3" />

        <TextView
            android:id="@+id/resiliencevalue_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="36dp"
            android:fontFamily="monospace"
            android:text="40"
            android:textColor="@color/DBOTextColor"
            android:textSize="36sp"
            app:layout_constraintEnd_toStartOf="@id/resilience_tab3"
            app:layout_constraintStart_toEndOf="@id/kinetic_tab3"
            app:layout_constraintTop_toBottomOf="@id/mobilityvalue_tab3" />

        <ImageButton
            android:id="@+id/recovery_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="163dp"
            android:background="?android:selectableItemBackground"
            android:foreground="@drawable/recovery"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/resiliencevalue_tab3"
            app:layout_constraintTop_toBottomOf="@id/resilience_tab3" />

        <TextView
            android:id="@+id/recoveryvalue_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="163dp"
            android:layout_marginTop="36dp"
            android:fontFamily="monospace"
            android:text="40"
            android:textColor="@color/DBOTextColor"
            android:textSize="36sp"
            app:layout_constraintEnd_toStartOf="@id/recovery_tab3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/resiliencevalue_tab3" />

        <ImageButton
            android:id="@+id/discipline_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="36dp"
            android:background="?android:selectableItemBackground"
            android:foreground="@drawable/discipline"
            app:layout_constraintEnd_toStartOf="@id/chest_tab3"
            app:layout_constraintStart_toEndOf="@id/disciplinevalue_tab3"
            app:layout_constraintTop_toBottomOf="@id/recovery_tab3" />

        <TextView
            android:id="@+id/disciplinevalue_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="36dp"
            android:fontFamily="monospace"
            android:text="40"
            android:textColor="@color/DBOTextColor"
            android:textSize="36sp"
            app:layout_constraintEnd_toStartOf="@id/discipline_tab3"
            app:layout_constraintStart_toEndOf="@id/energy_tab3"
            app:layout_constraintTop_toBottomOf="@id/recoveryvalue_tab3" />

        <ImageButton
            android:id="@+id/intellect_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="36dp"
            android:background="?android:selectableItemBackground"
            android:foreground="@drawable/intellect"
            app:layout_constraintEnd_toStartOf="@id/legs_tab3"
            app:layout_constraintStart_toEndOf="@id/intellectvalue_tab3"
            app:layout_constraintTop_toBottomOf="@id/discipline_tab3" />

        <TextView
            android:id="@+id/intellectvalue_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="36dp"
            android:fontFamily="monospace"
            android:text="40"
            android:textColor="@color/DBOTextColor"
            android:textSize="36sp"
            app:layout_constraintEnd_toStartOf="@id/intellect_tab3"
            app:layout_constraintStart_toEndOf="@id/power_tab3"
            app:layout_constraintTop_toBottomOf="@id/disciplinevalue_tab3" />

        <ImageButton
            android:id="@+id/strength_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="163dp"
            android:background="?android:selectableItemBackground"
            android:foreground="@drawable/strength"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/strengthvalue_tab3"
            app:layout_constraintTop_toBottomOf="@id/intellect_tab3" />

        <TextView
            android:id="@+id/strengthvalue_tab3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="163dp"
            android:layout_marginTop="36dp"
            android:fontFamily="monospace"
            android:text="40"
            android:textColor="@color/DBOTextColor"
            android:textSize="36sp"
            app:layout_constraintEnd_toStartOf="@id/strength_tab3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/intellectvalue_tab3" />

        <ImageView
            android:id="@+id/classicontab3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="37dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/warlockicon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/kinetic_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/pistol_icon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/classicontab3" />

        <ImageButton
            android:id="@+id/energy_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/hush_icon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/kinetic_tab3" />

        <ImageButton
            android:id="@+id/power_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/love_and_death_icon1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/energy_tab3" />

        <ImageButton
            android:id="@+id/ghost_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/omolon_shell_icon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/power_tab3" />

        <ImageButton
            android:id="@+id/helmet_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="37dp"
            android:src="@drawable/gensym_knight_casque_icon1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/arms_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="37dp"
            android:src="@drawable/gensym_knight_grips_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/helmet_tab3" />

        <ImageButton
            android:id="@+id/chest_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="37dp"
            android:src="@drawable/gensym_knight_cuirass_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/arms_tab3" />

        <ImageButton
            android:id="@+id/legs_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="37dp"
            android:src="@drawable/gensym_knight_strides_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/chest_tab3" />

        <ImageButton
            android:id="@+id/classitem_tab3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="37dp"
            android:src="@drawable/gensym_knight_cloak_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/legs_tab3" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>